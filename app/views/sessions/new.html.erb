<h1>Log in</h1>
<div id="form-outer">
  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Log in", class: "button" %>
  <% end %>
  <%= link_to "Don't have an account? Sign up here!", signup_path %>
</div>
